<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firstcodedemo">
    <!-- if you want to load images from the internet -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- if you want to load images from a file OR from the internet -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNoDialog">
        <activity android:name=".ui.selector.SelectorActivity"></activity>
        <activity android:name=".activity.FragmentActivity" />
        <activity android:name=".ui.category.CategoryActivity" />
        <activity android:name=".activity.PaintActivity" />
        <activity android:name=".ui.captcha.ImgCapthcaActivity" />
        <activity android:name=".activity.FruitActivity" />
        <activity android:name=".activity.MDActivity" />
        <activity android:name=".activity.MDRVActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ShowFragmentActivity" />
        <activity android:name=".activity.AbstractActivity" />
        <activity android:name=".activity.MyAbsActivity" />
    </application>

</manifest>