<?xml version="1.0" encoding="utf-8"?><!--自定义属性-->
<resources>
    <declare-styleable name="name">
        <attr name="title" format="string" />
        <attr name="titleTextSize" format="string" />
    </declare-styleable>

    <declare-styleable name="Selector">
        <attr name="text" format="string" />
        <attr name="img" format="reference" />
        <attr name="indicator" format="reference" />
        <attr name="text_size" format="integer" />
        <attr name="text_color" format="color" />
        <attr name="tag" format="reference" />
    </declare-styleable>

    <!--自定义Toolbar-->
    <declare-styleable name="TitleToolbar">
        <!--导航图标-->
        <attr name="nav_icon" format="reference" />
        <!--导航文本-->
        <attr name="nav_text" format="reference|string" />
    </declare-styleable>

    <!--搜索Toolbar-->
    <declare-styleable name="SearchToolbar">
        <!--导航图标-->
        <attr name="menu_icon" format="reference" />
    </declare-styleable>

</resources>